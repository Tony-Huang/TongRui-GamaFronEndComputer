            JDialog errDlg = new JDialog(this, false);
            JOptionPane.showMessageDialog(errDlg, "can't find device parameter: " + typeName, "Error Message", JOptionPane.ERROR_MESSAGE);
            errDlg.setVisible(true);
            return;
        }
